! special
*foreground:   #ffffff
!*background:   [70]#06213a
!*background:   [70]#003c6a
*background:   [70]#2e333a
*cursorColor:  #ffffff

!black
 *color0:  #252525
 *color8:  #505050
!red
 *color1:  #D4A7E2
 *color9:  #F99286
!green
 *color2:  #8DD78D
 *color10: #C3F786
!yellow
 *color3:  #EBEABD
 *color11: #FCFBCC
!blue
 !*color4:  #41B4A3
 *color4:  #81a2be
 *color12: #B6DEFB
!magenta
 !*color5:  #8E6794
 *color5:  #D57FD2
 *color13: #AD7FA8
!cyan
 *color6:  #C8C9EB
 *color14: #D7D9FC
!white
 *color7:  #FCFEFD
 *color15: #E2E2E2

! Fonts
URxvt*font: xft:SourceCodePro:size=11, \
            xft:Hack Nerd Font:style=Regular:size=11

! Cursors
Xcursor.theme: capitaine-cursors

!URxvt Clickable URLs
URxvt*perl-ext-common: default,matcher
URxvt*url-launcher: /usr/bin/xdg-open
URxvt*matcher.button: 1
URxvt*matcher.rend.0: Uline Bold color4

!Urxvt Transparent background
URxvt*depth: 32

!URxvt*inheritPixmap: true
!URxvt*transparent: true
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*shading: 50
!URxvt*tintColor: #a0bcdf 

Xft.dpi: 96
