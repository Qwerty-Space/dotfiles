{
    "workbench.colorTheme": "Tomorrow Night Blue",
    "files.hotExit": "off",
    "workbench.startupEditor": "none",
    "workbench.welcome.enabled": false,
    "window.restoreWindows": "none",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "vscode-mysql.enableTelemetry": false,
    "files.autoGuessEncoding": false,
    "editor.fontFamily": "Hasklig, Source Code Pro, Consolas, 'Courier New', monospace, 'Material Icons','Hack Nerd Font','nasin-nanpa'",
    "editor.fontLigatures": true,
    "window.openFoldersInNewWindow": "off",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.menuBarVisibility": "toggle",
    "files.eol": "\n",
    "python.pythonPath": "/usr/bin/python",
    "editor.multiCursorModifier": "alt",
    "python.formatting.provider": "autopep8",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.neovimInitPath": "/home/qwerty/.config/nvim/vscode.vim",
    "vscode-neovim.useCtrlKeysForInsertMode": false,
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "python.defaultInterpreterPath": "/usr/bin/python",
    "python.languageServer": "Jedi",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "workbench.editor.empty.hint": "hidden",
}